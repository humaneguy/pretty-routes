    "description": "Pretty routes for Laravel",
    "require": {
        "laravel/framework": "^8.12",
        "php": "^8.0",
        "orchestra/testbench": "6.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit":"^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Akinyosoye",
